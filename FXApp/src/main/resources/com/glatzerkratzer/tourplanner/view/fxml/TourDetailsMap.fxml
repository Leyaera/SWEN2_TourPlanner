<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>
<?import java.net.URL?>
<ScrollPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.glatzerkratzer.tourplanner.view.TourDetailsMapController"
          prefHeight="400.0" prefWidth="600.0">
    <content>
        <TextFlow>
            <Text fx:id="tourMapName"/>
            <Text fx:id="tourMapTransportType"/>
            <Text fx:id="tourMapDistance"/>
            <Text fx:id="tourMapDuration"/>
            <Text fx:id="tourMapStartTag" />
            <Text fx:id="tourMapStart"/>
            <Text fx:id="tourMapDestinationTag" />
            <Text fx:id="tourMapDestination"/>
        </TextFlow>
    </content>

    <stylesheets>
        <URL value="@../css/tourMapDescriptionStyle.css" />
    </stylesheets>

    <ImageView fx:id="tourMapImageView"/>

</ScrollPane>
